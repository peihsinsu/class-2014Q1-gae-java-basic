<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>GAE_Java_Day2</servlet-name>
		<servlet-class>com.gaeclass.day2.GAE_Java_Day2Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GAE_Java_Day2</servlet-name>
		<url-pattern>/gae_java_day2</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>QueueServlet</servlet-name>
		<servlet-class>com.gaeclass.day2.QueueServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QueueServlet</servlet-name>
		<url-pattern>/queue</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CronUrlServlet</servlet-name>
		<servlet-class>com.gaeclass.day2.CronUrlServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CronUrlServlet</servlet-name>
		<url-pattern>/cronurl</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
